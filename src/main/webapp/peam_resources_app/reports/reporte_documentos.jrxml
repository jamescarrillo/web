		<queryString language="SQL">
			<![CDATA[SELECT 
CASE 
WHEN DOCU_FECHA_DOCX IS NULL THEN '' ELSE
CASE 
WHEN LENGTH(DOCU_FECHA_DOCX)>2 THEN DOCU_FECHA_DOCX ELSE '' END
 END AS DOCU_FECHA_DOCX, 
DOCU_TITULO, DOCU_RESUMEN 
FROM WEB.DOCUMENTO 
WHERE DOCU_ESTADO = TRUE  
